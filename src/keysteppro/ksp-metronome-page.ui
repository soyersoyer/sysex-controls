<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <requires lib="libadwaita" version="1.0"/>
  <template class="KspMetronomePage" parent="ScNavigationPage">
    <property name="title" translatable="yes">Metronome</property>
    <property name="child">
      <object class="AdwToolbarView">
        <child type="top"><object class="AdwHeaderBar"/></child>
        <property name="content">
          <object class="ScPreferencesPage">

            <child>
              <object class="AdwPreferencesGroup">

                <child>
                  <object class="ScComboRow">
                    <child><object class="ArControl"><property name="id">0x4172</property></object></child>
                    <property name="title" translatable="yes">Count in</property>
                    <property name="model">
                      <object class="ScControlValueList">
                        <items>
                          <item>Off</item>
                          <item>1 Bar</item>
                          <item>2 Bars</item>
                        </items>
                      </object>
                    </property>
                  </object>
                </child>

              </object>
            </child>
            <child>
              <object class="AdwPreferencesGroup">

                <child>
                  <object class="ScComboRow">
                    <child><object class="ArControl"><property name="id">0x4174</property></object></child>
                    <property name="title" translatable="yes">Time division</property>
                    <property name="model">
                      <object class="ScControlValueList">
                        <items>
                          <item>1/4</item>
                          <item>1/4 Triplet</item>
                          <item>1/8</item>
                          <item>1/8 Triplet</item>
                          <item>1/16</item>
                          <item>1/16 Triplet</item>
                          <item>1/32</item>
                          <item>1/32 Triplet</item>
                        </items>
                      </object>
                    </property>
                  </object>
                </child>

                <child>
                  <object class="ScComboRow">
                    <child><object class="ArControl"><property name="id">0x4173</property></object></child>
                    <property name="title" translatable="yes">Time signature</property>
                    <property name="model">
                      <object class="ScControlValueList">
                        <items>
                          <item>1</item>
                          <item>2</item>
                          <item>3</item>
                          <item>4</item>
                          <item>5</item>
                          <item>6</item>
                          <item>7</item>
                          <item>8</item>
                          <item>9</item>
                          <item>10</item>
                          <item>11</item>
                          <item>12</item>
                          <item>13</item>
                          <item>14</item>
                          <item>15</item>
                          <item>16</item>
                        </items>
                      </object>
                    </property>
                  </object>
                </child>

              </object>
            </child>

          </object>
        </property>
      </object>
    </property>

  </template>
</interface>

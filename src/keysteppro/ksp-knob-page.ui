<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <requires lib="libadwaita" version="1.0"/>
  <template class="KspKnobPage" parent="ScNavigationPage">
    <property name="title" translatable="yes">Knobs</property>
    <property name="child">
      <object class="AdwToolbarView">
        <child type="top"><object class="AdwHeaderBar"/></child>
        <property name="content">
          <object class="ScPreferencesPage">

            <child>
              <object class="AdwPreferencesGroup">

                <child>
                  <object class="ScChRow">
                    <child><object class="ArControl"><property name="id">0x4150</property></object></child>
                    <property name="title" translatable="yes">Global Channel</property>
                  </object>
                </child>

              </object>
            </child>


            <child>
              <object class="KspKnob">
                <property name="title" translatable="yes">Pitch</property>
                <property name="control-id-offset">0x00</property>
              </object>
            </child>

            <child>
              <object class="KspKnob">
                <property name="title" translatable="yes">Gate</property>
                <property name="control-id-offset">0x01</property>
              </object>
            </child>

            <child>
              <object class="KspKnob">
                <property name="title" translatable="yes">Velocity</property>
                <property name="control-id-offset">0x02</property>
              </object>
            </child>

            <child>
              <object class="KspKnob">
                <property name="title" translatable="yes">Time Shift</property>
                <property name="control-id-offset">0x03</property>
              </object>
            </child>

            <child>
              <object class="KspKnob">
                <property name="title" translatable="yes">Randomness</property>
                <property name="control-id-offset">0x04</property>
              </object>
            </child>

          </object>
        </property>
      </object>
    </property>

  </template>
</interface>

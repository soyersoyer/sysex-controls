<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <requires lib="libadwaita" version="1.0"/>
  <template class="Amm2KeybedPage" parent="ScNavigationPage">
    <property name="title" translatable="yes">Keybed</property>
    <property name="child">
      <object class="AdwToolbarView">
        <child type="top"><object class="AdwHeaderBar"/></child>
        <property name="content">
          <object class="ScPreferencesPage">

            <child>
              <object class="AdwPreferencesGroup">

                <child>
                  <object class="ScChRow">
                    <child><object class="AkControl"><property name="id">0x01</property></object></child>
                  </object>
                </child>

              </object>
            </child>

            <child>
              <object class="AdwPreferencesGroup">

                <child>
                  <object class="AdwSpinRow">
                    <child>
                      <object class="AkControl">
                        <property name="id">0x02</property>
                        <property name="value-offset">-4</property>
                      </object>
                    </child>
                    <property name="title" translatable="yes">Octave</property>
                    <property name="adjustment">
                      <object class="GtkAdjustment">
                        <property name="lower">-4</property>
                        <property name="upper">4</property>
                        <property name="page-increment">2</property>
                        <property name="step-increment">1</property>
                      </object>
                    </property>
                  </object>
                </child>

                <child>
                  <object class="AdwSpinRow">
                    <child>
                      <object class="AkControl">
                        <property name="id">0x6c</property>
                        <property name="value-offset">-12</property>
                      </object>
                    </child>
                    <property name="title" translatable="yes">Transpose</property>
                    <property name="adjustment">
                      <object class="GtkAdjustment">
                        <property name="lower">-12</property>
                        <property name="upper">12</property>
                        <property name="page-increment">4</property>
                        <property name="step-increment">1</property>
                      </object>
                    </property>
                  </object>
                </child>

              </object>
            </child>

          </object>
        </property>
      </object>
    </property>

  </template>
</interface>

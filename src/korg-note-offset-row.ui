<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <requires lib="libadwaita" version="1.0"/>
  <template class="KorgNoteOffsetRow" parent="ScComboRow">
    <property name="title" translatable="yes">Note Offset</property>
    <property name="model">
      <object class="ScControlValueList">
        <items>
          <item>0 C</item>
          <item>+1 C#</item>
          <item>+2 D</item>
          <item>+3 D#</item>
          <item>+4 E</item>
          <item>+5 F</item>
          <item>+6 F#</item>
          <item>+7 G</item>
          <item>+8 G#</item>
          <item>+9 A</item>
          <item>+10 A#</item>
          <item>+11 B</item>
          <item>+12 C</item>
          </items>
      </object>
    </property>
  </template>
</interface>

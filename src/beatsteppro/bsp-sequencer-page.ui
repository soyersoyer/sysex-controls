                    </property>
                  </object>
                </child>

                <child>
                  <object class="AdwSwitchRow">
                    <child><object class="ArControl"><property name="id">0x6500</property></object></child>
                    <property name="title" translatable="yes">Triplet</property>
                  </object>
                </child>

                <child>
                  <object class="ScComboRow">
                    <child><object class="ArControl"><property name="id">0x5100</property></object></child>
                    <property name="title" translatable="yes">Scale Type</property>
                    <property name="model">

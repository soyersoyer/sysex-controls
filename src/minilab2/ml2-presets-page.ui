<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <requires lib="libadwaita" version="1.0"/>
  <template class="Ml2PresetsPage" parent="ScNavigationPage">
    <property name="title" translatable="yes">Presets</property>
    <property name="child">
      <object class="AdwToolbarView">
        <child type="top"><object class="AdwHeaderBar"/></child>
        <property name="content">
          <object class="ScPreferencesPage">

            <child>
              <object class="AdwPreferencesGroup">

               <child>
                  <object class="ScActionRow">
                    <property name="title" translatable="yes">Edit only in memory</property>
                    <property name="control-id-offset">0x000000</property>
                    <property name="target-page">Ml2PresetPage</property>
                    <signal name="activated" handler="sc_action_nav_push"/>
                  </object>
                </child>

                <child>
                  <object class="ScActionRow">
                    <property name="title" translatable="yes">Load / Store</property>
                    <signal name="activated" handler="ml2_book_on_presets_ar_presets_activated" swapped="yes"/>
                  </object>
                </child>

              </object>
            </child>

            <child>
              <object class="AdwPreferencesGroup">

               <child>
                  <object class="ScActionRow">
                    <property name="title" translatable="yes">Preset 2</property>
                    <property name="control-id-offset">0x020000</property>
                    <property name="target-page">Ml2PresetPage</property>
                    <signal name="activated" handler="sc_action_nav_push"/>
                  </object>
                </child>

                <child>
                  <object class="ScActionRow">
                    <property name="title" translatable="yes">Preset 3</property>
                    <property name="control-id-offset">0x030000</property>
                    <property name="target-page">Ml2PresetPage</property>
                    <signal name="activated" handler="sc_action_nav_push"/>
                  </object>
                </child>

                <child>
                  <object class="ScActionRow">
                    <property name="title" translatable="yes">Preset 4</property>
                    <property name="control-id-offset">0x040000</property>
                    <property name="target-page">Ml2PresetPage</property>
                    <signal name="activated" handler="sc_action_nav_push"/>
                  </object>
                </child>

                <child>
                  <object class="ScActionRow">
                    <property name="title" translatable="yes">Preset 5</property>
                    <property name="control-id-offset">0x050000</property>
                    <property name="target-page">Ml2PresetPage</property>
                    <signal name="activated" handler="sc_action_nav_push"/>
                  </object>
                </child>

                <child>
                  <object class="ScActionRow">
                    <property name="title" translatable="yes">Preset 6</property>
                    <property name="control-id-offset">0x060000</property>
                    <property name="target-page">Ml2PresetPage</property>
                    <signal name="activated" handler="sc_action_nav_push"/>
                  </object>
                </child>

                <child>
                  <object class="ScActionRow">
                    <property name="title" translatable="yes">Preset 7</property>
                    <property name="control-id-offset">0x070000</property>
                    <property name="target-page">Ml2PresetPage</property>
                    <signal name="activated" handler="sc_action_nav_push"/>
                  </object>
                </child>

                <child>
                  <object class="ScActionRow">
                    <property name="title" translatable="yes">Preset 8</property>
                    <property name="control-id-offset">0x080000</property>
                    <property name="target-page">Ml2PresetPage</property>
                    <signal name="activated" handler="sc_action_nav_push"/>
                  </object>
                </child>

              </object>
            </child>

          </object>
        </property>
      </object>
    </property>

  </template>
</interface>
